{
    "name": "deviantlab/tabulator-bundle",
    "description": "Tabulator Symfony bundle",
    "type": "symfony-bundle",
    "keywords": ["symfony-ux"],
    "version": "0.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Chunikhin",
            "email": "6insanes@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/flex": "^2.0",
        "symfony/twig-bundle": "^6.4|^7.0"
    },
    "require-dev": {
        "symfony/framework-bundle": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "DeviantLab\\TabulatorBundle\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": false
        }
    }
}
